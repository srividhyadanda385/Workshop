d={
    "vidhya": 20,
    "sneha": 14,
    "prem" : 90
}

name= input("Enter your name:")

if name in d:
    print("user exists.")
    print("Name:",name)
    print("Roll No:",d[name])
else:
    print("user does not exist.")
    num= int(input("Enter roll number:"))
    d[name]= num
    
    print("User added into the data.")
    print("Name:",name)
    print("Roll No:",d)
